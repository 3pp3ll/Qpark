{
  "expo": {
    "name": "Saffak - Smart Parking",
    "slug": "saffak-parking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "saffak",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-location",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Saffak to access your camera to scan QR codes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "permissions": [
      "android.permission.ACCESS_COARSE_LOCATION",
      "android.permission.ACCESS_FINE_LOCATION",
      "android.permission.CAMERA"
    ]
  }
}